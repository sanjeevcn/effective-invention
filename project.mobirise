range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\"></div>\n    <div class=\"two_background\"></div>\n    \n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-md-3 col-lg-4\" mbr-if=\"showLeftImage\">\n                <div class=\"image_left\">\n                    <img src=\"@PROJECT_PATH@/assets/images/bistrovertical6.jpg\" alt>\n                </div>\n            </div>\n            <div class=\"col-12\" mbr-class=\"{'col-md-6 col-lg-4' : showLeftImage || showRightImage}\">\n                <div class=\"main_header\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-1\">SPECIALS</h2>\n                    <div class=\"menu_widget\" mbr-list mbr-list-grow=\"listItems\">\n                        <div class=\"list_widget\">\n                            <h3 class=\"mbr-section-subtitle mb-0 mbr-fonts-style\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\">Italian pasta</h3>\n                            <p class=\"mbr-text mb-0 mbr-fonts-style\" data-app-selector=\".mbr-text, .linkBtn\" mbr-theme-style=\"display-4\">(Lorem ipsum dolor sit amet, consectetur) </p>\n                        </div>\n                    \n                        \n                    <div class=\"list_widget\">\n                            <h3 class=\"mbr-section-subtitle mb-0 mbr-fonts-style\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-5\">Pizza bonara</h3>\n                            <p class=\"mbr-text mb-0 mbr-fonts-style\" data-app-select